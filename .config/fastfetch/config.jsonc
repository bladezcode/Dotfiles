{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    //"recache": true,
    "type": "sixel",
    "source": "~/.config/fastfetch/images/girl.sixel",
    "width": 30,
    "padding": {
      //"left": 0,
      "right": 2
    }
    },
    "display": {
        
        "separator": ">  ",
        "color": {
            "separator": "39",
            "keys": "39",
            "title": "32"
        },
        "constants": [
            "───────────────────────────────────────────────────────────────────────────",
            "│\u001b[75C│\u001b[75D"
        ]
    },
    
    "modules": [
        {
            "format": "{#1}{#keys}╭{$1}╮\u001b[76D {user-name-colored}{at-symbol-colored}{host-name-colored} 🖥  ",
            "type": "title"
        },
        {
            "format": "{#1}{#keys}│                                                                           │",
            "type": "custom"
        },
        {
            "key": "{$2}{#32} kernel   ",
            "type": "kernel"
        },
        {
            "key": "{$2}{#32}󰅐 uptime   ",
            "type": "uptime"
        },
        {
            "key": "{$2}{#32}{icon} distro   ",
            "type": "os"
        },
        {
            "key": "{$2}{#32}󰇄 packages ",
            "type": "packages"
        },
        {
            "key": "{$2}{#32} term     ",
            "type": "terminal"
        },
        {
            "key": "{$2}{#32} shell    ",
            "type": "shell"
        },
        {
            "key": "{$2}{#32}󰍛 cpu      ",
            "type": "cpu",
            "showPeCoreCount": false,
            "temp": false
        },
        {
            "key": "{$2}{#32}󰍛 gpu      ",
            "type": "gpu"
        },
        {
            "key": "{$2}{#32}󰉉 disk     ",
            "type": "disk",
            "folders": "/"
        },
        {
            "key": "{$2}{#32} memory   ",
            "type": "memory"
        },
        {
            "key": "{$2}{#32}󰩟 network  ",
            "type": "localip",
            "format": "{ipv4} ({ifname})"
        },
        {
            "format": "{#1}{#keys}├{$1}┤",
            "type": "custom"
        },
        {
            "key": "{$2}{#32} colors   ",
            "type": "colors",
            "symbol": "circle"
        },
        {
            "format": "{#1}{#keys}╰{$1}╯",
            "type": "custom"
        }
    ]
}
